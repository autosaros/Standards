                      <PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/McInt/PortInterfaces/EnvironmentPIF/env_TLowLimit</PARAMETER-REF>
                    </PARAMETER-PROVIDE-COM-SPEC>
                    <PARAMETER-PROVIDE-COM-SPEC>
                      <INIT-VALUE>
                        <APPLICATION-VALUE-SPECIFICATION>
                          <CATEGORY>VALUE</CATEGORY>
                          <SW-VALUE-CONT>
                            <UNIT-REF DEST="UNIT">/McInt/Units/Kelvin</UNIT-REF>
                            <SW-VALUES-PHYS>
                              <V>10</V>
                            </SW-VALUES-PHYS>
                          </SW-VALUE-CONT>
                        </APPLICATION-VALUE-SPECIFICATION>
                      </INIT-VALUE>
                      <PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/McInt/PortInterfaces/EnvironmentPIF/env_TStepSize</PARAMETER-REF>
                    </PARAMETER-PROVIDE-COM-SPEC>
                    <PARAMETER-PROVIDE-COM-SPEC>
                      <INIT-VALUE>
                        <APPLICATION-VALUE-SPECIFICATION>
                          <CATEGORY>VALUE</CATEGORY>
                          <SW-VALUE-CONT>
                            <UNIT-REF DEST="UNIT">/McInt/Units/Kelvin</UNIT-REF>
                            <SW-VALUES-PHYS>
                              <V>200</V>
                            </SW-VALUES-PHYS>
                          </SW-VALUE-CONT>
                        </APPLICATION-VALUE-SPECIFICATION>
                      </INIT-VALUE>
                      <PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/McInt/PortInterfaces/EnvironmentPIF/env_THighLimitDistance</PARAMETER-REF>
                    </PARAMETER-PROVIDE-COM-SPEC>
                  </PROVIDED-COM-SPECS>
                  <PROVIDED-INTERFACE-TREF DEST="PARAMETER-INTERFACE">/McInt/PortInterfaces/EnvironmentPIF</PROVIDED-INTERFACE-TREF>
                </P-PORT-PROTOTYPE>
                <P-PORT-PROTOTYPE>
                  <SHORT-NAME>DtPPP</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">Time of one time step</L-2>
                  </DESC>
                  <PROVIDED-COM-SPECS>
                    <PARAMETER-PROVIDE-COM-SPEC>
                      <INIT-VALUE>
                        <APPLICATION-VALUE-SPECIFICATION>
                          <CATEGORY>VALUE</CATEGORY>
                          <SW-VALUE-CONT>
                            <UNIT-REF DEST="UNIT">/McInt/Units/Second</UNIT-REF>
                            <SW-VALUES-PHYS>
                              <V>0.1</V>
                            </SW-VALUES-PHYS>
                          </SW-VALUE-CONT>
                        </APPLICATION-VALUE-SPECIFICATION>
                      </INIT-VALUE>
                      <PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/McInt/PortInterfaces/DtPIF/Dt</PARAMETER-REF>
                    </PARAMETER-PROVIDE-COM-SPEC>
                  </PROVIDED-COM-SPECS>
                  <PROVIDED-INTERFACE-TREF DEST="PARAMETER-INTERFACE">/McInt/PortInterfaces/DtPIF</PROVIDED-INTERFACE-TREF>
                </P-PORT-PROTOTYPE>
              </PORTS>
              <DATA-TYPE-MAPPING-REFS>
                <DATA-TYPE-MAPPING-REF DEST="DATA-TYPE-MAPPING-SET">/McInt/DataTypeMappings/DataTypeMappingSet</DATA-TYPE-MAPPING-REF>
              </DATA-TYPE-MAPPING-REFS>
            </PARAMETER-SW-COMPONENT-TYPE>
          </ELEMENTS>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>

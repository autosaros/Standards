                        <ARGUMENT-DATA-PROTOTYPE>
                          <SHORT-NAME>id</SHORT-NAME>
                          <DESC>
                            <L-2 L="EN">The Transfer ID of the Software Package.</L-2>
                          </DESC>
                          <TYPE-TREF DEST="STD-CPP-IMPLEMENTATION-DATA-TYPE">/AUTOSAR/UpdateAndConfigurationManagement/ImplementationDataTypes_Blueprint/TransferIdType</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE>
                          <SHORT-NAME>progress</SHORT-NAME>
                          <DESC>
                            <L-2 L="EN">The progress of the current package processing (0% - 100%). 0x00 ... 0x64, 0xFF for "‘No information available"’</L-2>
                          </DESC>
                          <TYPE-TREF DEST="STD-CPP-IMPLEMENTATION-DATA-TYPE">/AUTOSAR/StdTypes/uint8_t</TYPE-TREF>
                          <DIRECTION>OUT</DIRECTION>
                          <VARIATION-POINT>
